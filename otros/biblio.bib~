@ARTICLE{JKRBPaM,
  author = {Kennedy, J. and Mendes, R.},
  title = {{Neighborhood topologies in fully informed and Best of Neighborhood
	Particle Swarms}},
  journal = {IEEE Transactions on Systems, Man, and Cybernetics, Part C: Applications
	and Reviews.},
  year = {2006},
  volume = {36},
  pages = {515--519},
  number = {4},
  abstract = {{In this study, we vary the way an individual in the particle swarm
	interacts with its neighbors. The performance of an individual depends
	on population topology as well as algorithm version. It appears that
	a fully informed particle swarm is more susceptible to alterations
	in the topology, but with a good topology, it can outperform the
	canonical version.}},
  booktitle = {Systems, Man, and Cybernetics, Part C: Applications and Reviews,
