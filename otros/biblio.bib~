@INCOLLECTION{FonPaqLop06:hypervolume,
  author = {Carlos M. {Fonseca} and Lu{\'i}s {Paquete} and Manuel {L{\'o}pez-Ib{\'a}{\~n}ez}},
  title = {{An Improved Dimension\hspace{0pt}-\hspace{0pt}Sweep Algorithm for
	the Hypervolume Indicator}},
  booktitle = {Proceedings of the 2006 Congress on Evolutionary Computation (CEC
	2006)},
  publisher = {IEEE Press},
  year = {2006},
  pages = {1157-1163},
  address = {Piscataway, NJ},
  abstract = {This paper presents a recursive, dimension-sweep algorithm for computing
	the hypervolume indicator of the quality of a set of $n$ non-dominated
	points in $d>2$ dimensions. It improves upon the existing HSO (Hypervolume
	by Slicing Objectives) algorithm by pruning the recursion tree to
	avoid repeated dominance checks and the recalculation of partial
	hypervolumes. Additionally, it incorporates a recent result for the
	three-dimensional special case. The proposed algorithm achieves $O(n^{d-2}
	\log n)$ time and linear space complexity in the worst-case, but
	experimental results show that the pruning techniques used may reduce
	the time complexity exponent even further.},
  crossref = {CEC2006},
  doi = {10.1109/CEC.2006.1688440}
}

