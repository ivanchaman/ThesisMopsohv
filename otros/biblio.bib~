@ARTICLE{Maurice11,
  author = {Clerc, M. and Kennedy, J.},
  title = {{The Particle Swarm: Explosion, Stability, and Convergence in a Multidimensional
	Complex Space}},
  journal = {Evolutionary Computation, IEEE Transactions on},
  year = {2002},
  volume = {6},
  pages = {58--73},
  number = {1},
  abstract = {{The particle swarm is an algorithm for finding optimal regions of
	complex search spaces through the interaction of individuals in a
	population of particles. This paper analyzes a particle's trajectory
	as it moves in discrete time (the algebraic view), then progresses
	to the view of it in continuous time (the analytical view). A five-dimensional
	depiction is developed, which describes the system completely. These
	analyses lead to a generalized model of the algorithm, containing
	a set of coefficients to control the system's convergence tendencies.
	Some results of the particle swarm optimizer, implementing modifications
	derived from the analysis, suggest methods for altering the original
	algorithm in ways that eliminate problems and increase the ability
	of the particle swarm to find optima of some well-studied test functions}},
  booktitle = {Evolutionary Computation, IEEE Transactions on},
  keywords = {ant, complex\_system, swarm},
  posted-at = {2006-09-01 14:38:46},
  priority = {3},
  url = {http://dx.doi.org/10.1109/4235.985692}
}

